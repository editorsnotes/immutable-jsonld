{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "generators": true,
    "jsx": false,
    "modules": true,
    "spread": true,
    "templateStrings": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "comma-dangle": 0,
    "comma-style": [2, "first"],
    "curly": [2, "multi-line"],
    "eol-last": 2,
    "jsx-quotes": 2,
    "no-console": 1,
    "no-multiple-empty-lines": [2, {"max": 1}],
    "no-trailing-spaces": [2],
    "no-use-before-define": [2, "nofunc"],
    "semi": [2, "never"]
  }
}
